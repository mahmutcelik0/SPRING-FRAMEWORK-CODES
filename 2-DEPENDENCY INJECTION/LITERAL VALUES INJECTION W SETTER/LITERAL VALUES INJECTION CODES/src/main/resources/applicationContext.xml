<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="myFortuneService" class="com.example.demo.CLASSES.HelloFortuneService"/>


    <bean id="footballCoach" class="com.example.demo.CLASSES.FootballCoach">

        <property name="fortuneService" ref="myFortuneService"/>
        <property name="coachName" value="MURAT OSMAN UNALIR"/>
        <property name="coachMail" value="unalir@gmail.com"/>
        <property name="coachAge" value="51"/>

    </bean>


</beans>





